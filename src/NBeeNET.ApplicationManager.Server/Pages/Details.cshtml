@page
@model NBeeNET.ApplicationManager.Server.Pages.DetailsModel
@{
    ViewData["Title"] = "Details";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1> @ViewData["AppName"] </h1>
<br>
<h2> 应用配置 </h2>
<br>
<p class="text-left">
    <h8>API接口 : </h8>
    <a href="api/Configuration?appname=@ViewData["AppName"]" target="_blank">api/Configuration?appname=@ViewData["AppName"]&key=</a><br><br>
    Key :
    @foreach (var app in (string[])ViewData["ConfigurationList"])
    {
        <span>@app.Replace(".json", "").Split("/")[app.Replace(".json", "").Split("/").Length - 1] </span><span>&nbsp;</span><br>
    }
</p>

<br>

<h2> 发布版本 </h2>
<br>
<h8> 当前版本 </h8>
<p class="text-left">
    <h8>API接口 : </h8>
    <a href="api/Version?appname=@ViewData["AppName"]" target="_blank">api/Version?appname=@ViewData["AppName"]</a>
</p>
<br>
<h8> 历史版本 </h8>
<p class="text-left">
    @foreach (var app in (string[])ViewData["VersionList"])
    {
        @app.Split("/")[app.Split("/").Length - 1] <br>
    }
</p>